{
    "name": "@efffrida/sql",
    "version": "0.0.12",
    "description": "effect-ts sql-frida",
    "homepage": "https://github.com/leonitousconforti/efffrida",
    "repository": {
        "type": "git",
        "url": "https://github.com/leonitousconforti/efffrida.git",
        "directory": "packages/sql"
    },
    "license": "GPL-3.0-only",
    "author": "Leo Conforti <leo@leoconforti.us> (https://leoconforti.us)",
    "type": "module",
    "exports": {
        "./package.json": "./package.json",
        ".": "./src/index.ts",
        "./*": "./src/*.ts",
        "./internal/*": null
    },
    "scripts": {
        "build": "pnpm build-esm && pnpm build-annotate && pnpm build-cjs && build-utils pack-v3",
        "build-annotate": "babel build/esm --plugins annotate-pure-calls --out-dir build/esm --source-maps",
        "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
        "build-esm": "tsc -b tsconfig.build.json",
        "check": "tsc -b tsconfig.json",
        "codegen": "build-utils prepare-v3",
        "coverage": "vitest --coverage",
        "test": "vitest"
    },
    "dependencies": {
        "@opentelemetry/semantic-conventions": "1.37.0"
    },
    "devDependencies": {
        "@effect/cluster": "0.50.4",
        "@effect/experimental": "0.56.0",
        "@effect/platform": "0.92.1",
        "@effect/platform-node": "0.98.3",
        "@effect/rpc": "0.71.0",
        "@effect/sql": "0.46.0",
        "@effect/vitest": "0.26.0",
        "@effect/workflow": "0.11.3",
        "@efffrida/frida-tools": "workspace:*",
        "@efffrida/platform": "workspace:*",
        "@efffrida/rpc": "workspace:*",
        "@types/frida-gum": "19.0.1",
        "effect": "3.18.4",
        "vitest": "3.2.4"
    },
    "peerDependencies": {
        "@effect/experimental": "0.56.0",
        "@effect/platform": "0.92.1",
        "@effect/sql": "0.46.0",
        "effect": "3.18.4"
    },
    "publishConfig": {
        "access": "public",
        "directory": "dist",
        "linkDirectory": false
    }
}
