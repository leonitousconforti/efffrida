{
    "name": "@efffrida/frida-tools",
    "version": "0.0.1",
    "description": "Frida with effect-ts",
    "homepage": "https://github.com/leoconforti/efffrida",
    "repository": {
        "type": "git",
        "url": "https://github.com/leoconforti/efffrida.git",
        "directory": "packages/frida-tools"
    },
    "license": "GPL-3.0-only",
    "author": "Leo Conforti <leo@leoconforti.us> (https://leoconforti.us)",
    "type": "module",
    "scripts": {
        "build": "pnpm build-esm && pnpm build-annotate && pnpm build-cjs && build-utils pack-v2",
        "build-annotate": "babel build/esm --plugins annotate-pure-calls --out-dir build/esm --source-maps",
        "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
        "build-esm": "tsc -b tsconfig.build.json",
        "check": "tsc -b tsconfig.json",
        "codegen": "build-utils prepare-v2"
    },
    "dependencies": {
        "frida": "16.7.9"
    },
    "devDependencies": {
        "@effect/cluster": "0.29.12",
        "@effect/platform": "0.80.7",
        "@effect/platform-node": "0.76.12",
        "@effect/rpc": "0.55.10",
        "@effect/sql": "0.33.7",
        "@effect/vitest": "0.20.7",
        "@efffrida/frida-compile": "workspace:^",
        "@types/frida-gum": "18.8.2",
        "effect": "3.14.7",
        "vitest": "3.1.1"
    },
    "peerDependencies": {
        "@effect/platform": "0.80.7",
        "effect": "3.14.7"
    },
    "publishConfig": {
        "access": "public",
        "directory": "dist"
    }
}
