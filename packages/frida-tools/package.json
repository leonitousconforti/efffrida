{
    "name": "@efffrida/frida-tools",
    "version": "0.0.24",
    "description": "Frida with effect-ts",
    "keywords": [
        "frida.re",
        "reverse engineering",
        "effect-ts"
    ],
    "homepage": "https://github.com/leonitousconforti/efffrida",
    "repository": {
        "type": "git",
        "url": "https://github.com/leonitousconforti/efffrida.git",
        "directory": "packages/frida-tools"
    },
    "license": "GPL-3.0-only",
    "author": "Leo Conforti <leo@leoconforti.us> (https://leoconforti.us)",
    "sideEffects": [],
    "type": "module",
    "exports": {
        "./package.json": "./package.json",
        ".": "./src/index.ts",
        "./*": "./src/*.ts",
        "./internal/*": null
    },
    "files": [
        "src/**/*.ts",
        "dist/**/*.js",
        "dist/**/*.js.map",
        "dist/**/*.d.ts",
        "dist/**/*.d.ts.map"
    ],
    "scripts": {
        "build": "tsc -b tsconfig.build.json && babel dist --plugins annotate-pure-calls --out-dir dist --source-maps",
        "check": "tsc -b tsconfig.json",
        "codegen": "build-utils prepare-v4",
        "coverage": "vitest --coverage",
        "test": "vitest"
    },
    "dependencies": {
        "frida": "17.5.2"
    },
    "devDependencies": {
        "@effect/cluster": "0.56.1",
        "@effect/experimental": "0.58.0",
        "@effect/platform": "0.94.1",
        "@effect/platform-node": "0.104.0",
        "@effect/rpc": "0.73.0",
        "@effect/sql": "0.49.0",
        "@effect/vitest": "0.27.0",
        "@effect/workflow": "0.16.0",
        "@efffrida/polyfills": "workspace:*",
        "@types/frida-gum": "19.0.1",
        "@types/node": "25.0.6",
        "effect": "3.19.14",
        "vitest": "4.0.17"
    },
    "peerDependencies": {
        "@effect/platform": "0.94.1",
        "effect": "3.19.14"
    },
    "publishConfig": {
        "access": "public",
        "exports": {
            "./package.json": "./package.json",
            ".": "./dist/index.js",
            "./*": "./dist/*.js",
            "./internal/*": null
        }
    },
    "tags": [
        "frida.re",
        "reverse engineering",
        "effect-ts"
    ]
}
