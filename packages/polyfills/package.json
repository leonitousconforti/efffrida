{
    "name": "@efffrida/polyfills",
    "version": "0.0.3",
    "description": "polyfills for using effect in frida's GumJS environment",
    "keywords": [
        "frida.re",
        "reverse engineering",
        "effect-ts"
    ],
    "homepage": "https://github.com/leonitousconforti/efffrida",
    "repository": {
        "type": "git",
        "url": "https://github.com/leonitousconforti/efffrida.git",
        "directory": "packages/polyfills"
    },
    "license": "GPL-3.0-only",
    "author": "Leo Conforti <leo@leoconforti.us> (https://leoconforti.us)",
    "sideEffects": true,
    "type": "module",
    "exports": {
        "./package.json": "./package.json",
        ".": "./src/index.ts",
        "./*": null
    },
    "files": [
        "src/**/*.ts",
        "dist/**/*.js",
        "dist/**/*.js.map",
        "dist/**/*.d.ts",
        "dist/**/*.d.ts.map"
    ],
    "scripts": {
        "build": "tsc -b tsconfig.build.json && babel dist --plugins annotate-pure-calls --out-dir dist --source-maps",
        "check": "tsc -b tsconfig.json",
        "codegen": "build-utils prepare-v4"
    },
    "dependencies": {
        "abort-controller": "3.0.0",
        "core-js": "3.47.0",
        "event-target-polyfill": "0.0.4",
        "fast-text-encoding": "1.0.6"
    },
    "devDependencies": {
        "@types/frida-gum": "19.0.1",
        "@types/node": "25.0.6"
    },
    "publishConfig": {
        "access": "public",
        "exports": {
            "./package.json": "./package.json",
            ".": "./dist/index.js",
            "./*": null
        }
    },
    "tags": [
        "frida.re",
        "reverse engineering",
        "effect-ts"
    ]
}
