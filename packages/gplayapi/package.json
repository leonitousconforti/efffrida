{
    "name": "@efffrida/gplayapi",
    "version": "0.0.1",
    "description": "Google play api",
    "keywords": [
        "frida.re",
        "reverse engineering",
        "effect-ts"
    ],
    "homepage": "https://github.com/leonitousconforti/efffrida",
    "repository": {
        "type": "git",
        "url": "https://github.com/leonitousconforti/efffrida.git",
        "directory": "packages/gplayapi"
    },
    "license": "GPL-3.0-only",
    "author": "Leo Conforti <leo@leoconforti.us> (https://leoconforti.us)",
    "sideEffects": [],
    "type": "module",
    "exports": {
        "./package.json": "./package.json",
        ".": "./src/index.ts",
        "./*": "./src/*.ts",
        "./internal/*": null,
        "./generated/*": null
    },
    "files": [
        "src/**/*.ts",
        "dist/**/*.js",
        "dist/**/*.js.map",
        "dist/**/*.d.ts",
        "dist/**/*.d.ts.map",
        "protobuf/**/*.proto",
        "devices/**/*.properties"
    ],
    "scripts": {
        "build": "tsc -b tsconfig.build.json && babel dist --plugins annotate-pure-calls --out-dir dist --source-maps",
        "check": "tsc -b tsconfig.json",
        "codegen": "build-utils prepare-v4",
        "protobuf": "buf generate"
    },
    "dependencies": {
        "properties-file": "3.6.1"
    },
    "devDependencies": {
        "@bufbuild/buf": "1.59.0",
        "@bufbuild/protobuf": "2.10.0",
        "@effect/cluster": "0.52.1",
        "@effect/experimental": "0.57.0",
        "@effect/platform": "0.93.0",
        "@effect/platform-node": "0.100.0",
        "@effect/rpc": "0.72.1",
        "@effect/sql": "0.48.0",
        "@effect/workflow": "0.12.1",
        "@types/node": "24.10.0",
        "effect": "3.19.0"
    },
    "peerDependencies": {
        "@bufbuild/protobuf": "2.10.0",
        "@effect/platform": "0.93.0",
        "effect": "3.19.0"
    },
    "publishConfig": {
        "access": "public",
        "exports": {
            "./package.json": "./package.json",
            ".": "./dist/index.ts",
            "./*": "./dist/*.ts",
            "./internal/*": null,
            "./generated/*": null
        }
    },
    "tags": [
        "frida.re",
        "reverse engineering",
        "effect-ts"
    ]
}
