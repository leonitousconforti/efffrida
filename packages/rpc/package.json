{
    "name": "@efffrida/rpc",
    "version": "0.0.7",
    "description": "effect rpc-frida",
    "homepage": "https://github.com/leonitousconforti/efffrida",
    "repository": {
        "type": "git",
        "url": "https://github.com/leonitousconforti/efffrida.git",
        "directory": "packages/rpc"
    },
    "license": "GPL-3.0-only",
    "author": "Leo Conforti <leo@leoconforti.us> (https://leoconforti.us)",
    "type": "module",
    "exports": {
        "./package.json": "./package.json",
        ".": "./src/index.ts",
        "./*": "./src/*.ts",
        "./internal/*": null
    },
    "scripts": {
        "build": "pnpm build-esm && pnpm build-annotate && pnpm build-cjs && build-utils pack-v3",
        "build-annotate": "babel build/esm --plugins annotate-pure-calls --out-dir build/esm --source-maps",
        "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
        "build-esm": "tsc -b tsconfig.build.json",
        "check": "tsc -b tsconfig.json",
        "codegen": "build-utils prepare-v3",
        "coverage": "vitest --coverage",
        "test": "vitest"
    },
    "dependencies": {
        "@efffrida/frida-tools": "workspace:^"
    },
    "devDependencies": {
        "@effect/cluster": "0.30.6",
        "@effect/experimental": "0.44.18",
        "@effect/platform": "0.80.18",
        "@effect/platform-node": "0.77.6",
        "@effect/rpc": "0.56.6",
        "@effect/sql": "0.33.18",
        "@effect/vitest": "0.21.1",
        "@efffrida/frida-compile": "workspace:^",
        "@efffrida/platform": "workspace:*",
        "@types/frida-gum": "18.8.2",
        "effect": "3.14.18",
        "vitest": "3.1.2"
    },
    "peerDependencies": {
        "@effect/platform": "0.80.18",
        "@effect/rpc": "0.56.6",
        "effect": "3.14.18"
    },
    "publishConfig": {
        "access": "public",
        "directory": "dist",
        "linkDirectory": false
    }
}
