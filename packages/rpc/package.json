{
    "name": "@efffrida/rpc",
    "version": "0.0.18",
    "description": "effect rpc-frida",
    "keywords": [
        "frida.re",
        "reverse engineering",
        "effect-ts"
    ],
    "homepage": "https://github.com/leonitousconforti/efffrida",
    "repository": {
        "type": "git",
        "url": "https://github.com/leonitousconforti/efffrida.git",
        "directory": "packages/rpc"
    },
    "license": "GPL-3.0-only",
    "author": "Leo Conforti <leo@leoconforti.us> (https://leoconforti.us)",
    "sideEffects": [],
    "type": "module",
    "exports": {
        "./package.json": "./package.json",
        "./node": "./src/node/index.ts",
        "./frida": "./src/frida/index.ts",
        "./*": "./src/*.ts",
        "./internal/*": null,
        "./shared/*": null,
        "./*/index": null
    },
    "files": [
        "src/**/*.ts",
        "dist/**/*.js",
        "dist/**/*.js.map",
        "dist/**/*.d.ts",
        "dist/**/*.d.ts.map"
    ],
    "scripts": {
        "build": "tsc -b tsconfig.build.json && babel dist --plugins annotate-pure-calls --out-dir dist --source-maps",
        "check": "tsc -b tsconfig.json",
        "codegen": "build-utils prepare-v4",
        "coverage": "vitest --coverage",
        "test": "vitest"
    },
    "dependencies": {
        "@efffrida/frida-tools": "workspace:^",
        "@efffrida/platform": "workspace:*",
        "@efffrida/polyfills": "workspace:*"
    },
    "devDependencies": {
        "@effect/cluster": "0.56.0",
        "@effect/experimental": "0.58.0",
        "@effect/platform": "0.94.0",
        "@effect/platform-node": "0.104.0",
        "@effect/rpc": "0.73.0",
        "@effect/sql": "0.49.0",
        "@effect/vitest": "0.27.0",
        "@effect/workflow": "0.16.0",
        "@efffrida/vitest-pool": "workspace:*",
        "@types/frida-gum": "19.0.1",
        "@types/node": "25.0.3",
        "effect": "3.19.13",
        "frida": "17.5.1",
        "vitest": "4.0.16"
    },
    "peerDependencies": {
        "@effect/platform": "0.94.0",
        "@effect/rpc": "0.73.0",
        "effect": "3.19.13"
    },
    "publishConfig": {
        "access": "public",
        "exports": {
            "./package.json": "./package.json",
            "./node": "./dist/node/index.js",
            "./frida": "./dist/frida/index.js",
            "./*": "./dist/*.js",
            "./internal/*": null,
            "./shared/*": null,
            "./*/index": null
        }
    },
    "tags": [
        "frida.re",
        "reverse engineering",
        "effect-ts"
    ]
}
