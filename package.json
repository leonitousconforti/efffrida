{
    "private": true,
    "type": "module",
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "build": "tsc -b tsconfig.build.json && pnpm --recursive --parallel run build",
        "changeset-publish": "pnpm build && TEST_DIST= pnpm vitest && changeset publish",
        "changeset-version": "changeset version && node scripts/version.mjs",
        "check": "tsc -b tsconfig.json",
        "check-recursive": "pnpm --recursive exec tsc -b tsconfig.json",
        "clean": "node scripts/clean.mjs",
        "codegen": "pnpm --recursive --parallel run codegen",
        "coverage": "vitest --coverage",
        "lint": "eslint \"**/{src,test,examples,scripts,dtslint}/**/*.{ts,mjs}\"",
        "lint-fix": "pnpm lint --fix",
        "test": "vitest"
    },
    "devDependencies": {
        "@babel/cli": "7.27.0",
        "@babel/core": "7.26.10",
        "@babel/plugin-transform-export-namespace-from": "7.25.9",
        "@babel/plugin-transform-modules-commonjs": "7.26.3",
        "@changesets/changelog-github": "0.5.1",
        "@changesets/cli": "2.28.1",
        "@effect/build-utils": "0.7.9",
        "@effect/language-service": "0.5.1",
        "@effect/vitest": "^0.20.5",
        "@eslint/js": "9.23.0",
        "@types/frida-gum": "18.8.2",
        "@types/node": "22.14.0",
        "@typescript-eslint/eslint-plugin": "8.29.0",
        "@typescript-eslint/parser": "8.29.0",
        "babel-plugin-annotate-pure-calls": "0.5.0",
        "effect": "3.14.5",
        "eslint": "9.23.0",
        "eslint-config-prettier": "10.1.1",
        "eslint-import-resolver-typescript": "4.3.1",
        "eslint-plugin-codegen": "0.30.0",
        "eslint-plugin-import-x": "4.10.0",
        "eslint-plugin-prettier": "5.2.6",
        "eslint-plugin-simple-import-sort": "12.1.1",
        "eslint-plugin-sort-destructure-keys": "2.0.0",
        "glob": "11.0.1",
        "prettier": "3.5.3",
        "prettier-plugin-jsdoc": "1.3.2",
        "prettier-plugin-packagejson": "2.5.10",
        "tsx": "4.19.3",
        "typescript": "5.8.2",
        "typescript-eslint": "8.29.0",
        "vitest": "3.1.1"
    },
    "packageManager": "pnpm@9.10.0"
}
