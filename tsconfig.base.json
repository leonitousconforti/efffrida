{
  "compilerOptions": {
    "strict": true,
    "exactOptionalPropertyTypes": true,
    "moduleDetection": "force",
    "composite": true,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "esModuleInterop": false,
    "declaration": true,
    "skipLibCheck": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "NodeNext",
    "lib": ["ES2022"],
    "types": [],
    "isolatedModules": true,
    "sourceMap": true,
    "declarationMap": true,
    "noImplicitReturns": false,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "noEmitOnError": false,
    "noErrorTruncation": false,
    "allowJs": false,
    "checkJs": false,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUncheckedIndexedAccess": false,
    "strictNullChecks": true,
    "baseUrl": ".",
    "target": "ES2022",
    "module": "NodeNext",
    "incremental": true,
    "removeComments": false,
    "plugins": [{ "name": "@effect/language-service" }],
    "paths": {
      "@template/effect-frida": ["./packages/effect-frida/src/index.js"],
      "@template/effect-frida/*": ["./packages/effect-frida/src/*.js"],
      "@template/effect-frida/test/*": ["./packages/effect-frida/test/*.js"],
      "@template/effect-frida-il2cpp-bridge": ["./packages/effect-frida-il2cpp-bridge/src/index.js"],
      "@template/effect-frida-il2cpp-bridge/*": ["./packages/effect-frida-il2cpp-bridge/src/*.js"],
      "@template/effect-frida-il2cpp-bridge/test/*": ["./packages/effect-frida-il2cpp-bridge/test/*.js"],
      "@template/effect-rpc-frida": ["./packages/effect-rpc-frida/src/index.js"],
      "@template/effect-rpc-frida/*": ["./packages/effect-rpc-frida/src/*.js"],
      "@template/effect-rpc-frida/test/*": ["./packages/effect-rpc-frida/test/*.js"]
    }
  }
}
